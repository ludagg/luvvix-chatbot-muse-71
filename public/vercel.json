
{
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "/auth"
    },
    {
      "source": "/api/:path*",
      "destination": "https://luvvix-id.com/api/:path*"
    },
    {
      "source": "/ai/:path*",
      "destination": "https://luvvi-ai.vercel.app/:path*"
    },
    {
      "source": "/medecine/:path*",
      "destination": "https://medivox.vercel.app/:path*"
    },
    {
      "source": "/streammix/:path*",
      "destination": "https://luvvi-streammix.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*.luvvix.it.com"
        }
      ]
    }
  ]
}
