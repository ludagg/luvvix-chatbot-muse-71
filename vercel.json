{
  "rewrites": [
    {
      "source": "/ai/:path*",
      "destination": "https://luvvi-ai.vercel.app/:path*"
    },
    {
      "source": "/medecine/:path*",
      "destination": "https://appmedivox.vercel.app/:path*"
    },
    {
      "source": "/streammix/:path*",
      "destination": "https://luvvi-streammix.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}